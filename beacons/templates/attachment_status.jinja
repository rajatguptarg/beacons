{% extends "base.jinja" %}

{% block title %} Status {% endblock %}
   
{% block content%}
  {{ status }}
  {% for key in status%}
        {% if key == 'data' %}
            <strong>Data:</strong> {{ status[key] }}<br>
        {% endif %}
        {% if key == 'attachmentName' %}
            <strong>Attachment Name:</strong> {{ status[key] }}<br>
        {% endif %}
        {% if key == 'namespacedType' %}
            <strong>Namespace:</strong> {{ status[key] }}<br>
        {% endif %}
  {% endfor %}	

{% endblock %}
